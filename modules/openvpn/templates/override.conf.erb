[Service]

# Enable ip forwarding
ExecStartPre=/usr/sbin/sysctl -w net.ipv4.ip_forward=1

# Remove the vpn route we get from openstack since this is the vpn server if already gone then exit 0 for systemd
ExecStartPre=/usr/bin/bash -c '/sbin/ip route del 10.8.0.32/27 via 10.8.0.101 dev eth0 || exit 0'
